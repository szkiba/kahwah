{
  "name": "kahwah",
  "version": "0.1.2",
  "description": "Test runner for k6, inspired by Mocha",
  "keywords": [
    "workflow",
    "k6"
  ],
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "homepage": "https://github.com/szkiba/kahwah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szkiba/kahwah.git"
  },
  "bugs": {
    "url": "https://github.com/szkiba/kahwah/issues"
  },
  "author": "Ivan Szkiba",
  "scripts": {
    "build": "webpack",
    "lint": "eslint .",
    "docs": "typedoc",
    "test": "find test -name '*js' | xargs -n 1 k6 run --no-usage-report",
    "samples": "find samples -name '*js' | xargs -n 1 k6 run --no-usage-report"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "babel-loader": "^8.2.2",
    "context": "^1.1.3",
    "core-js": "^3.12.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "k6": "0.0.0",
    "prettier": "^2.3.0",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {}
}
